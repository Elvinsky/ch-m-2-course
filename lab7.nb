Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"M1", "=", 
   RowBox[{
    RowBox[{"Maximize", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"f", "[", "x", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", 
            RowBox[{"NN", "+", "1"}]}], "}"}]}], "]"}], ",", 
        RowBox[{
         SubscriptBox["x", "0"], "\[LessEqual]", "x", "\[LessEqual]", 
         SubscriptBox["x", "n"]}]}], "}"}], ",", "x"}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"M2", "=", 
   RowBox[{
    RowBox[{"Minimize", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"f", "[", "x", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", 
            RowBox[{"NN", "+", "1"}]}], "}"}]}], "]"}], ",", 
        RowBox[{
         SubscriptBox["x", "0"], "\[LessEqual]", "x", "\[LessEqual]", 
         SubscriptBox["x", "n"]}]}], "}"}], ",", "x"}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"M", "=", 
   RowBox[{"Max", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Abs", "[", "M1", "]"}], ",", 
      RowBox[{"Abs", "[", "M2", "]"}]}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Omega]", "[", "t_", "]"}], "=", 
   RowBox[{
    UnderoverscriptBox["\[Product]", 
     RowBox[{"i", "=", "0"}], "n"], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"t", "-", 
       SubscriptBox["x", "i"]}], ")"}], 
     RowBox[{"m", "+", "1"}]]}]}], ";", 
  RowBox[{
   SubscriptBox["M", "\[Omega]"], "=", 
   RowBox[{
    RowBox[{"Maximize", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Abs", "[", 
         RowBox[{"\[Omega]", "[", "x", "]"}], "]"}], ",", 
        RowBox[{
         SubscriptBox["x", "0"], "\[LessEqual]", "x", "\[LessEqual]", 
         SubscriptBox["x", "n"]}]}], "}"}], ",", "x"}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"R", "=", 
  RowBox[{
   RowBox[{
    FractionBox["M", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"NN", "+", "1"}], ")"}], "!"}]], 
    SubscriptBox["M", "\[Omega]"]}], "//", "N"}]}]}], "Input",
